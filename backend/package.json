  "version": "1.0.0",
  "description": "GraphQL API server for survey app",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc && prisma generate",
    "prod": "npm run build && npm run start",
