{
  "name": "backend",
  "version": "1.0.0",
  "scripts": {
    "predev": "prisma generate --schema=src/prisma/schema.prisma",
    "dev": "ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "predev:debug": "prisma generate",
    "dev:debug": "cross-env DEBUG=prisma:* node --inspect -r ts-node/register server.ts",
    "postinstall": "prisma generate --schema=src/prisma/schema.prisma",
    "prisma:generate": "prisma generate --schema=src/prisma/schema.prisma"
  },
  "dependencies": {
    "@graphql-tools/schema": "^10.0.0",
    "@prisma/client": "^5.11.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.0",
    "graphql": "^16.8.1",
    "graphql-yoga": "^5.0.0",
    "jsonwebtoken": "^9.0.2",
    "typescript": "^5.0.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20.17.46",
    "cross-env": "^7.0.3",
    "prisma": "^5.11.0",
    "ts-node": "^10.9.0"
  }
}
